<?xml version = 1.0?>
<package>
    <metadata>
        <id>{{ language.params.packageName }}</id>
        <version>{{ sdk.version }}</version>
        <authors>{{ spec.contactName }} {{'<'}}{{ spec.contactEmail }}{{'>'}}</authors>
        <owners>{{ spec.contactName }} {{'<'}}{{ spec.contactEmail }}{{'>'}}</owners>
        <licenseUrl>{{ sdk.license }}</licenseUrl>
        <projectUrl>{{ sdk.gitURL }}</projectUrl>
        <iconUrl></iconUrl>
        <requireLicenseAcceptance>true</requireLicenseAcceptance>
        <description>{{spec.description}}</description>
        <releaseNotes></releaseNotes>
        <copyright></copyright>
        <tags></tags>
        <dependencies>
        </dependencies>
        <files>
            <file src="src/sdk.cs" target="" />
        </files>
    </metadata>
</package>